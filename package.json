		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.11",
		"tailwindcss": "^4.1.11"
